<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>ControladorVenta</servlet-name>
        <servlet-class>servlet.ControladorVenta</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ControladorLogin</servlet-name>
        <servlet-class>servlet.ControladorLogin</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ServicioServlet</servlet-name>
        <servlet-class>servlet.ServicioServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ProductoServlet</servlet-name>
        <servlet-class>servlet.ProductoServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>DetalleServicioServlet</servlet-name>
        <servlet-class>servlet.DetalleServicioServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ServletFiador</servlet-name>
        <servlet-class>servlet.ServletFiador</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Servletcliente</servlet-name>
        <servlet-class>servlet.Servletcliente</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ServletCargo</servlet-name>
        <servlet-class>servlet.ServletCargo</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>servletempleado</servlet-name>
        <servlet-class>servlet.servletempleado</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>ControladorVenta</servlet-name>
        <url-pattern>/ControladorVenta</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ControladorLogin</servlet-name>
        <url-pattern>/ControladorLogin</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ServicioServlet</servlet-name>
        <url-pattern>/ServicioServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ProductoServlet</servlet-name>
        <url-pattern>/ProductoServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>DetalleServicioServlet</servlet-name>
        <url-pattern>/DetalleServicioServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ServletFiador</servlet-name>
        <url-pattern>/ServletFiador</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Servletcliente</servlet-name>
        <url-pattern>/Servletcliente</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ServletCargo</servlet-name>
        <url-pattern>/ServletCargo</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>servletempleado</servlet-name>
        <url-pattern>/servletempleado</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
